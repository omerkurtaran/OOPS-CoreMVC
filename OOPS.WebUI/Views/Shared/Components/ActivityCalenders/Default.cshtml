@model IEnumerable<OOPS.DTO.Static.ActivityCalenderDTO>

    <ul class="list-group-item">
        <li class="list-group-item  align-items-center">
            <span class="caption-subject font-dark bold "><i class="icon-calendar"></i> Etkinlik Takvimi </span>
        </li>
        @foreach (var item in Model)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">

                <div class="row">
                    <div class="col-md-8">@item.Name</div>
                    <div class="col-md-4">@item.DateTime</div>
                </div>
            </li>
        }
    </ul>